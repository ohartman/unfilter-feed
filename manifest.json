{
  "manifest_version": 3,
  "name": "UnfilteredFeed v2",
  "version": "1.0.0",
  "description": "See what YouTube's algorithm is hiding from you",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "UnfilteredFeed"
  }
}
